{
    "display_information": {
        "name": "IT Support Bot",
        "description": "IT Support Bot by Multifactor LLP",
        "background_color": "#000000"
    },
    "features": {
        "app_home": {
            "home_tab_enabled": false,
            "messages_tab_enabled": true,
            "messages_tab_read_only_enabled": false
        },
        "bot_user": {
            "display_name": "IT Support Bot",
            "always_online": true
        },
        "slash_commands": [
            {
                "command": "/it-help",
                "description": "Get IT Support",
                "should_escape": false
            },
            {
                "command": "/it-admin",
                "description": "Admin Dashboard",
                "should_escape": false
            }
        ]
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "commands",
                "chat:write",
                "chat:write.public",
                "users:read",
                "users.profile:read",
                "im:write"
            ]
        }
    },
    "settings": {
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": false,
        "token_rotation_enabled": false
    }
}